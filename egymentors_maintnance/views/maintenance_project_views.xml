        <field name="res_model">maintenance.task.categ</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Add a new maintenance task category
            </p>
        </field>
    </record>


    <!--Task Line-->
    <!-- Task Line: Form View -->
    <record id="maintenance_task_line_form_view" model="ir.ui.view">
        <field name="name">maintenance.task.line.form.view</field>
        <field name="model">maintenance.task.line</field>
        <field name="arch" type="xml">
            <form string="Task Sheet Line" create="0" edit="0" delete="0">
