<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
    
        <record id="view_users_form_internal_requisition" model="ir.ui.view">
            <field name="name">res.users.form.inherit</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">                
                <xpath expr="//notebook[last()]" position="inside">
                    <page string="Internal Requisitions" name="internal_requisitions">
                        <group string="Internal Requisitions" groups="base.group_no_one">
                            <group>
                                <field name="internal_requisition_ids" widget="many2many_tags" readonly="1"/>
                            </group>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
        
    </data>
</odoo>